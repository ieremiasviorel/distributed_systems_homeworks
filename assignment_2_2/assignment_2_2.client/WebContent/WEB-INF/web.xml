<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">

	<servlet>
		<servlet-name>TaxValueServiceServlet</servlet-name>
		<servlet-class>org.assignment_2_2.client.communication.TaxValueServiceServlet</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>ResalePriceServiceServlet</servlet-name>
		<servlet-class>org.assignment_2_2.client.communication.ResalePriceServiceServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>TaxValueServiceServlet</servlet-name>
		<url-pattern>/tax-value</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>ResalePriceServiceServlet</servlet-name>
		<url-pattern>/resale-price</url-pattern>
	</servlet-mapping>

</web-app>